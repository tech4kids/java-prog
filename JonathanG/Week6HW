import java.util.*;
class primefinder {
	public static void main(String[] args) {
	input();
	}

	public static void input() {

	Scanner console = new Scanner(System.in);

        System.out.print("This is the upper bound: ");
        long top = console.nextLong();
        System.out.println(2)
	long number = 1;
	int f = 0;
	int result;
	while( number < top) {
		number = number + 2;
		long e = 1;
		long number1 = number;
		double a = number;
		while( e < number) {
			e = e + 2;
			result = number % e;
			if( result == 0) {
				break;
				}
			if( e > Math.sqrt(a)) {
				System.out.println(number1);
				f = f +1
				break;
				}
			}
		}
		System.out.println("Percentage:" + f/top);
	}
}
